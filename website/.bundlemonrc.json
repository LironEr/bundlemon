{
  "verbose": true,
  "baseDir": "./build",
  "files": [
    {
      "path": "index.html"
    },
    {
      "path": "main.<hash>.js"
    },
    {
      "friendlyName": "JS files",
      "path": "**/*.<hash>.js"
    }
  ],
  "groups": [
    {
      "friendlyName": "JS files",
      "path": "**/*.js"
    },
    {
      "friendlyName": "Images",
      "path": "**/*.png"
    }
  ],
  "defaultCompression": "none",
  "reportOutput": ["github"]
}
