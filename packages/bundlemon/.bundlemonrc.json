{
  "verbose": true,
  "baseDir": "./lib",
  "files": [
    {
      "path": "**/*.js"
    }
  ],
  "groups": [
    {
      "path": "**/*"
    }
  ],
  "defaultCompression": "none",
  "reportOutput": [
    [
      "github-pr",
      {
        "statusCheck": true,
        "prComment": true
      }
    ]
  ]
}
